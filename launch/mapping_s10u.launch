<launch>
<!-- Launch file for Livox MID360 LiDAR -->

	<arg name="rviz" default="true" />

	<rosparam command="load" file="$(find fast_lio)/config/s10u.yaml" />
	<param name="feature_extract_enable" type="bool" value="1"/>
	<param name="point_filter_num" type="int" value="1"/>
	<param name="max_iteration" type="int" value="10" />
	<param name="filter_size_surf" type="double" value="0.1" />
	<param name="filter_size_map" type="double" value="0.1" />
	<param name="cube_side_length" type="double" value="1000" />
	<param name="runtime_pos_log_enable" type="bool" value="0" />
	<param name="root_dir" type="string" value="$(find fast_lio)"/>
  <node pkg="fast_lio" type="fastlio_mapping" name="laserMapping_s10u" output="screen" required="true" />

	<group if="$(arg rviz)">
	<node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz_s10u" args="-d $(find fast_lio)/rviz_cfg/fast_lio_s10u.rviz" />
	</group>

</launch>
